<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.LogDao">

    <insert id="log">
        insert into tbl_log(info,createDate)
        values
        (#{info},now())
    </insert>
</mapper>